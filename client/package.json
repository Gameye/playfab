    "tape": "^4.11.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "ping": "^0.2.2",
    "playfab-sdk": "^2.40.191218",
    "tslib": "^1.10.0"
  },
  "engines": {
    "node": "^12"
